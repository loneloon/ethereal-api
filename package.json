{
  "name": "ethereal-api",
  "version": "1.0.0",
  "description": "Unified authentication API",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "start": "node ./dist/index.js",
    "init-prisma-sqlite": "npx prisma init --datasource-provider sqlite",
    "migrate-prisma": "npx prisma migrate dev --name init",
    "wipe-db": "rm -r ./prisma/migrations && rm ./prisma/dev.db && rm ./prisma/dev.db-journal"
  },
