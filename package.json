  "license": "MIT",
  "devDependencies": {
    "@prisma/client": "^4.14.1",
    "@types/lodash": "^4.14.194",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "prisma": "^4.14.1"
  }
}
